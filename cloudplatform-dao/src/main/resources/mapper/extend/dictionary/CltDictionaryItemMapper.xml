<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.northking.cloudplatform.dao.dictionary.CltDictionaryItemMapper" >


    <select id="queryDictionaryItemByTypeId" resultType="net.northking.cloudplatform.domain.dictionary.CltDictionaryItem" parameterType="java.lang.String">

 SELECT * FROM CLT_DICTIONARY_ITEM  AS A WHERE A.TYPE_ID = #{typeId,jdbcType=VARCHAR} ORDER BY ORDER_SEQ

    </select>






</mapper>